{% extends 'frontoffice.html.twig' %}

{% block body %}
    <section class="trophey-room">
        <div class="container">
            <div class="row row-offcanvas row-offcanvas-left">
                <div class="news-list col-xs-12 col-md-1">
                </div>
                {% for article in articles %}
                <div class="news-list col-xs-12 col-md-5">
                    <div class="item img-top">
                        <div class="img-wrap">
                            <div class="bage"><a href="#">CATEGORIE</a></div>
                            <a href="#"><img src="{{ asset('Commun/images/TMG.png') }}" alt="post image"></a>
                        </div>
                        <div class="info">
                            <a href="#" class="name">{{ article.title }}</a>
                            <div class="wrap">
                                <a href="#">{{ article.createDate|date("m/d/Y") }}</a>
                            </div>
                            <div class="clear"></div>
                        </div>
                    </div>
                </div>
                {% endfor %}
            </div>
        </div>
    </section>
{% endblock %}